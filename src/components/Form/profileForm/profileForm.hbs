<form>
  <div class="profile-container__userinfo">
    {{{emailInput}}}
    {{{loginInput}}}
    {{{firstNameInput}}}
    {{{secondNameInput}}}
    {{{displayNameInput}}}
    {{{phoneInput}}}
  </div>
  <div class="profile-container__buttons">
    {{#if isChangeForm}}
      {{{saveButton}}}
    {{else}}
      {{{changeInfoLink}}}
      {{{changePasswordLink}}}
      {{{logOutLink}}}
    {{/if}}
  </div>
</form>